{
  "name": "next-start",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --no-engine && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "postinstall": "prisma generate --no-engine",
    "clean": "rimraf .next",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run format:check",
    "check-all": "npm run format:check && npm run lint && npm run type-check",
    "fix-all": "npm run format && npm run lint:fix"
  },
